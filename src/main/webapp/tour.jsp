  <script type="text/javascript">    
    jQuery(document).ready(function(){      
      jQuery("#list").click(function(){    
        jQuery(".prod-box").animate({opacity:0},function(){
          jQuery(".grid").removeClass("grid-active");
          jQuery(".list").addClass("list-active");
          jQuery(".prod-box").attr("class", "prod-box-list shadow");
          jQuery(".prod-box-list").stop().animate({opacity:1});
        });
      });
  
      jQuery("#grid").click(function(){  
        jQuery(".prod-box-list").animate({opacity:0},function(){
          jQuery(".list").removeClass("list-active");
          jQuery(".grid").addClass("grid-active");
          jQuery(".prod-box-list").attr("class", "prod-box shadow");
          jQuery(".prod-box").stop().animate({opacity:1});
        });
      });
      
      jQuery(".prod-box-title, .tour-details-icon").click(function(){
        var prodBoxInfoDiv = jQuery(this).closest("div.prod-box-info");
        if( !prodBoxInfoDiv.length ) {
          return;
        }
        
        var tourDetailsDiv = jQuery(prodBoxInfoDiv).find(".tour-details");
        if( !tourDetailsDiv.length ) {
          return;
        }
        
        jQuery.fancybox({
          content: tourDetailsDiv,
          openEffect : "elastic",
          closeEffect : "elastic",
          closeBtn  : true
        });
      });
      
      jQuery(".tour-gallery, .tour-gallery-icon").click(function() {
        var prodBoxDiv = jQuery(this).closest("div.shadow");
        if( !prodBoxDiv.length ) {
          return;
        }
        
        var hrefArr = new Array();
        var hrefPrefix = "http://file.rentalcarmalaysia.com.my/tour/";
        var imgName = jQuery(prodBoxDiv).find(".imgName").text();
        if( imgName != "" ) {
          var imgNo, strIndex, hrefUrl;
          var imgCount = parseInt(jQuery(prodBoxDiv).find(".imgCount").text());
          for(var i = 0; i < imgCount; i++) {
            imgNo = i + 1;
            if( imgNo < 10 ) {
              strIndex = "0" + imgNo;
            }
            else {
              strIndex = "" + imgNo;
            }
            
            hrefUrl = hrefPrefix + imgName + "/" + imgName + strIndex + ".jpg";
            hrefArr[i] = {"href": hrefUrl};
          }
        }
        
        if( hrefArr.length > 0 ) {
          jQuery.fancybox.open(hrefArr, {
            openEffect : "elastic",
            closeEffect : "elastic",
            prevEffect : "elastic",
            nextEffect : "elastic",
            closeBtn  : true,
            helpers : {
              title : {
                type : "inside"
              }
            },
            afterLoad : function() {
              this.title = "Image " + (this.index + 1) + " of " + this.group.length + (this.title ? " - " + this.title : "");
            }
          });
        }
      });
    });
  </script>
